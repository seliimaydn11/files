int t=100;


void setup() {
 pinMode(13,OUTPUT);
 pinMode(12,OUTPUT);
 pinMode(11,OUTPUT);
 pinMode(10,OUTPUT);
 

}

void loop() {


 digitalWrite(13,HIGH); delay(t);
 digitalWrite(13,LOW); delay(t);

 digitalWrite(12,HIGH); delay(t);
 digitalWrite(12,LOW); delay(t);

 digitalWrite(11,HIGH); delay(t);
 digitalWrite(11,LOW); delay(t);

 digitalWrite(10,HIGH); delay(t);
 digitalWrite(10,LOW); delay(t);
    


 digitalWrite(10,HIGH); delay(t);
 digitalWrite(10,LOW); delay(t);

 digitalWrite(11,HIGH); delay(t);
 digitalWrite(11,LOW); delay(t);

 digitalWrite(12,HIGH); delay(t);
 digitalWrite(12,LOW); delay(t);

 digitalWrite(13,HIGH); delay(t);
 digitalWrite(13,LOW); delay(t);

}
